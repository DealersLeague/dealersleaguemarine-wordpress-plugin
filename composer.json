{
  "name": "dealersleague/dealersleague-marine",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/DealersLeague/dealersleaguemarine-sdk.git"
    }
    ],
  "license": "MIT License",
  "authors": [
    {
      "name": "Walter Barcelos",
      "email": "walter@dealersleague.com",
      "homepage": "https://dealersleague.com/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "dealersleague\\marine\\": ""
    }
  },
  "description": "Connection to Dealers League Marine API",
  "homepage": "https://dealersleague.com/",
  "keywords": [
    "wordpress",
    "plugin",
    "api",
    "dealersleague",
    "marine"
  ],
  "require": {
    "php": ">5.3.0",
    "dealersleague/dealersleaguemarine-sdk": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "rpkamp/fancy-testdox-printer": "^0.2.5",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "*"
  },
  "scripts": {
    "pot": "wp i18n make-pot . languages/dealer-league-marine.pot",
    "test": "phpunit --colors=always --printer \"rpkamp\\FancyTestdoxPrinter\"",
    "setup": [
      "phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs",
      "phpcs --config-set default_standard WordPress"
    ],
    "error": "phpcs --extensions=php",
    "fix": "phpcbf --extensions=php"
  },
  "suggest": {
    "composer/installers": "~1.0"
  },
  "type": "wordpress-plugin"
}